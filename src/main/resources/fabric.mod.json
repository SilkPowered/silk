{
  "schemaVersion": 1,
  "id": "silk",
  "version": "${version}",

  "name": "Silk",
  "description": "Silk is a fabric mod provides compatiblity with spigot plugins.",
  "authors": [
    "qyl27"
  ],
  "contributors": [
  ],
  "contact": {
    "homepage": "https://github.com/SilkPowered/silk",
    "issues": "https://github.com/SilkPowered/silk/issues"
  },

  "license": "GPLv3",
  "icon": "assets/silk/icon.png",

  "environment": "server",
  "entrypoints": {
    "preLaunch": [
      "cx.rain.silk.SilkLoader"
    ],
    "main": [
      "cx.rain.silk.SilkMod"
    ],
    "mm:early_risers": [
      "cx.rain.silk.early_loading.SilkEarlyLoader"
    ]
  },
  "mixins": [
    "silk.mixins.json"
  ],
  "accessWidener": "silk.accessWidener",

  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": "~1.19",
    "java": ">=17"
  }
}
